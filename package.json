{
  "name": "eclogite",
  "version": "0.0.0",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "start": "npm-run-all -p serve:*",
    "ng": "ng",
    "serve:ng": "webpack-dev-server --port=4200 --config webpack.config.dev.js",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "prebuild": "trash dist && node script/generate-env.js production",
    "build": "npm-run-all build:*",
    "build:webpack": "webpack --config webpack.config.prod.js",
    "prebuild:electron": "tsc main.ts --outDir dist && copyfiles package.json dist",
    "build:electron": "cd dist && npm install --production & cd .. && node script/package.js",
    "postbuild": "node script/generate-env.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "preserve:electron": "node script/generate-env.js && tsc main.ts",
    "serve:electron": "electron .",
    "test": "karma start ./karma.conf.js"
  },
  "private": true,
  "dependencies": {
    "dugite": "1.53.0",
    "js-sha256": "^0.9.0"
  },
  "devDependencies": {
    "@angular/animations": "^5.1.2",
